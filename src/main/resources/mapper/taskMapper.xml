<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
namespace: 命名空间，用于标识每一个Mapper XML文件中的语句，预防在不同的Mapper XML文件中存在相同的语句ID
-->
<mapper namespace="com.mlh.mapper.TaskDao">
    <insert id="save" useGeneratedKeys="true" keyProperty="task.taskId">
        insert into task(task_id) values (#{task.taskId});
    </insert>
</mapper>